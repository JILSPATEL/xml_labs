<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema">
    <element name="juicers">
        <complexType>
            <sequence>
                <element name="juicer" minOccurs="0" maxOccurs="unbounded">
                    <complexType>
                        <sequence>
                                <element name="name" type="string"/>
                                <element name="image" type="string"/>
                                <element name="description" type="string"/>
                                <element name="warranty" type="string" minOccurs="0"/>
                                <element name="weight" type="string" minOccurs="0"/>
                                <element name="cost" type="string" minOccurs="1" maxOccurs="unbounded"/>
                                <element name="retailer" type="string"/>
                        </sequence>
                    </complexType>
                </element>
            </sequence>
        </complexType>
    </element>
</schema>